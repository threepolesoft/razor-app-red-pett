                        <span class="checkmark"></span>
                        <p class="radio-btn-label">
                            National/Central
                        </p>
                    </label>
                    <label for="radio-2" class="radio-btn-wrap">
                        <input type="radio" id="radio-2" class="hidden radio-btn-input" value="1" name="radio" @onclick=@(() => FacilityLevel = "Regional")>
